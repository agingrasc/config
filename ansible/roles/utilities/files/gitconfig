[push]
	default = current
[core]
	autocrlf = input
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[status]
	showUntrackedFiles = no
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[branch]
	autosetupmerge = always
[pull]
	rebase = false
[init]
	defaultBranch = main
